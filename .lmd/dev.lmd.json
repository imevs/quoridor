{
    "name"   : "dev build",
    "root"   : "../",
    "output" : "dev.lmd.js",
    "modules": {
        "underscore"       : {
            "path"   : "libs/underscore.js",
            "exports": "_"
        },
        "jQuery"           : {
            "path"   : "libs/jquery-1.10.2.js",
            "exports": "$"
        },
        "Backbone"         : {
            "path"   : "libs/backbone.js",
            "require": ["underscore", "jQuery"],
            "exports": "Backbone"
        },
        "raphael"          : {
            "path"   : "libs/raphael.js",
            "require": "jQuery"
        },
        "backbone.raphael" : "libs/backbone.raphael.js",
        "utils"            : "js/utils.js",
        "FieldModel"       : {
            "path"   : "js/models/FieldModel.js",
            "exports": "FieldModel"
        },
        "FenceHModel"      : {
            "path"   : "js/models/FenceModel.js",
            "exports": "FenceHModel"
        },
        "FenceVModel"      : {
            "path"   : "js/models/FenceModel.js",
            "exports": "FenceVModel"
        },
        "FieldsCollection" : {
            "path"   : "js/models/FieldModel.js",
            "exports": "FieldsCollection"
        },
        "FencesCollection" : {
            "require": {"FieldModel": "FieldModel"},
            "path"   : "js/models/FenceModel.js",
            "exports": "FencesCollection"
        },
        "PlayerModel"      : {
            "path"   : "js/models/PlayerModel.js",
            "exports": "PlayerModel"
        },
        "PlayersCollection": {
            "path"   : "js/models/PlayerModel.js",
            "exports": "PlayersCollection"
        },
        "BoardModel"       : {
            "require": {
                "utils": "utils",
                "FenceHModel": "FenceHModel", "FenceVModel": "FenceVModel",
                "FieldModel": "FieldModel", "PlayerModel": "PlayerModel",
                "FieldsCollection": "FieldsCollection",
                "FencesCollection": "FencesCollection",
                "PlayersCollection": "PlayersCollection"
            },
            "path"   : "js/models/BoardModel.js",
            "exports": "BoardModel"
        },
        "BoardView" : {
            "require": {
                "GameObject": "GameObject", "InfoView": "InfoView",
                "FieldView": "FieldView", "FenceView": "FenceView",
                "PlayerView": "PlayerView"
            },
            "path"   : "js/views/BoardView.js",
            "exports": "BoardView"
        },
        "GameObject": {
            "require": ["backbone.raphael", "raphael"],
            "path"   : "js/views/GameObject.js",
            "exports": "GameObject"
        },
        "InfoView"  : {
            "path"   : "js/views/InfoView.js",
            "exports": "InfoView",
            "require": "game-info"
        },
        "FieldView" : {
            "require": {"GameObject":"GameObject"},
            "path"   : "js/views/FieldView.js",
            "exports": "FieldView"
        },
        "FenceView" : {
            "require": {
                "GameObject": "GameObject",
                "FenceHModel": "FenceHModel",
                "FenceVModel": "FenceVModel"
            },
            "path"   : "js/views/FenceView.js",
            "exports": "FenceView"
        },
        "PlayerView": {
            "require": {"GameObject":"GameObject"},
            "path"   : "js/views/PlayerView.js",
            "exports": "PlayerView"
        },

        "<%= file%>": "templates/*.html",

        "main": {
            "path"   : "js/app.js",
            "require": {
                "Backbone": "Backbone",
                "underscore": "_",
                "jQuery": "$",
                "BoardView": "BoardView",
                "BoardModel":"BoardModel"
            }
        }
    },

    "www_root"        : ".",
    "sourcemap"       : "dev.lmd.map",
    "sourcemap_inline": true,
    "sourcemap_www"   : "",

    "main": "main"
}